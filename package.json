    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.9",
    "chai": "^4.1.0",
    "deep-diff": "^0.3.8",
    "mocha": "^3.4.2",
    "object-no-new-keys": "^1.0.3",
    "ts-node": "^3.2.1",
